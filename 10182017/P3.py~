#!/usr/bin/env Python3

# Test data
# DNA = ''

DNA = 'GATGGGATTGGGGTTTTCCCC'

# calculate AT content
Anumbers = DNA.count ('A')
Tnumbers = DNA.count ('T')


Gnumbers= DNA.count ('G')
Cnumbers= DNA.count ('C')

print ( "There are " , Anumbers , "of 'A's in this DNA and there are", Tnumbers ,"of T's in this DNA so there are a total of", Anumbers+Tnumbers , "A's and T's in this DNA")
print ( "There are " , Cnumbers , "of 'C's in this DNA and there are", Gnumbers ,"of G's in this DNA so there are a total of", Cnumbers+Gnumbers , "C's and G's in this DNA")
